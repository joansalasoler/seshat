msgid "Evaluate algebraic query"
msgstr "Avalua aquesta consulta algebraica"

#: seshat/resources/config.json:32
msgid "Evaluate this algebraic expression"
msgstr "Avalua aquesta expressió algebraica"

#: seshat/resources/config.json:39
msgid "Transform to uppercase"
msgstr "Transforma a majúscules"

#: seshat/resources/config.json:46
msgid "Transform to lowercase"
msgstr "Transforma a minúscules"

#: seshat/resources/config.json:53
msgid "Capitalize first letter of each word"
msgstr "Converteix a majúscula inicial cada mot"

#: seshat/resources/config.json:60
msgid "Trim whitespace"
msgstr "Suprimeix els espais en blanc"

#: seshat/resources/config.json:67
msgid "Capitalize first letter of each line"
msgstr "Converteix a majúscula inicial cada línia"

#: seshat/resources/config.json:74
msgid "Delete duplicate lines"
msgstr "Elimina les línies duplicades"

#: seshat/resources/config.json:81
msgid "Delete empty lines"
msgstr "Elimina les línies buides"

